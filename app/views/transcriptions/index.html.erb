<p id="notice"><%= notice %></p>

<h1 class="text-center">Transcriptions</h1>

<div class="digg_pagination">
  <div class="page_info">
    <%= page_entries_info @posts %>
  </div>
  <%= will_paginate @transcriptions, :container => false %>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th colspan="1"></th>
      <th>Question</th>
      <th>Pattern</th>
      <th>Transcription</th>
      <th>Locked</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @transcriptions %>
  </tbody>
</table>

<%= will_paginate %>

